        <botDialogGroup>Application_Status</botDialogGroup>
        <botSteps>
            <botMessages>
                <message>Here are the next steps as your application is being processed.</message>
            </botMessages>
            <type>Message</type>
        </botSteps>
        <botSteps>
            <botMessages>
                <message>The Checklist Completion for you application is {!ApplicationChecklistCompletion}

The step {!ActionPlanName} is currently listed as {!ActionPlanStatus}.</message>
            </botMessages>
            <type>Message</type>
        </botSteps>
        <botSteps>
            <botVariableOperation>
                <botInvocation>
                    <invocationActionName>R_A_Bot_Knowledge_Variable_Application_In_Progress</invocationActionName>
                    <invocationActionType>flow</invocationActionType>
                    <invocationMappings>
                        <parameterName>varKnowledgeArticleSearch</parameterName>
                        <type>Output</type>
                        <variableName>AppDeadlineQuestion</variableName>
