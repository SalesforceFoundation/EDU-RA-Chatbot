                    </invocationMappings>
                </botInvocation>
                <type>Set</type>
            </botVariableOperation>
            <type>VariableOperation</type>
        </botSteps>
        <botSteps>
            <conversationRecordLookup>
                <SObjectType>Knowledge__kav</SObjectType>
