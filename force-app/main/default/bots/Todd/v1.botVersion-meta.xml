                    <fieldName>Knowledge__kav.Title</fieldName>
                </lookupFields>
                <lookupFields>
                    <fieldName>Knowledge__kav.Summary</fieldName>
                </lookupFields>
                <lookupFields>
                    <fieldName>Knowledge__kav.Chat_Bot_URL__c</fieldName>
                </lookupFields>
                <lookupFields>
                    <fieldName>Knowledge__kav.UrlName</fieldName>
                </lookupFields>
                <maxLookupResults>3</maxLookupResults>
                <sourceVariableName>AppDeadlineQuestion</sourceVariableName>
                <sourceVariableType>ConversationVariable</sourceVariableType>
                <targetVariableName>AppDeadlineKnowledgeResults</targetVariableName>
            </conversationRecordLookup>
            <type>RecordLookup</type>
        </botSteps>
        <botSteps>
            <botVariableOperation>
                <botMessages>
                    <message>Here are the articles I found that may help you as your application is being processed. Select from the following if you would like to learn more:</message>
                </botMessages>
                <botVariableOperands>
                    <disableAutoFill>true</disableAutoFill>
                    <sourceName>_Object</sourceName>
                    <sourceType>StandardMlSlotClass</sourceType>
                    <targetName>Knowledge</targetName>
                    <targetType>ConversationVariable</targetType>
                </botVariableOperands>
                <invalidInputBotNavigation>
                    <botNavigationLinks>
                        <targetBotDialog>Confused</targetBotDialog>
                    </botNavigationLinks>
                    <type>Redirect</type>
                </invalidInputBotNavigation>
                <optionalCollect>false</optionalCollect>
                <quickReplyOptionTemplate>{!Title}</quickReplyOptionTemplate>
                <quickReplyType>Dynamic</quickReplyType>
                <quickReplyWidgetType>Buttons</quickReplyWidgetType>
                <sourceVariableName>AppDeadlineKnowledgeResults</sourceVariableName>
                <sourceVariableType>ConversationVariable</sourceVariableType>
                <type>Collect</type>
            </botVariableOperation>
            <type>VariableOperation</type>
        </botSteps>
        <botSteps>
            <botMessages>
                <message>Selected Article Title: {!Knowledge.Title}

Selected Article Summary: {!Knowledge.Summary}

You can find out more here: {!Knowledge.Chat_Bot_URL__c}</message>
            </botMessages>
            <type>Message</type>
        </botSteps>
        <botSteps>
            <botMessages>
                <message>Is there anything else I can assist you with?</message>
            </botMessages>
            <type>Message</type>
        </botSteps>
        <botSteps>
            <botNavigation>
                <botNavigationLinks>
                    <targetBotDialog>Something_Else</targetBotDialog>
                </botNavigationLinks>
                <botNavigationLinks>
                    <targetBotDialog>Speak_with_an_Admissions_Counselor</targetBotDialog>
                </botNavigationLinks>
                <botNavigationLinks>
                    <targetBotDialog>End_Chat</targetBotDialog>
                </botNavigationLinks>
                <type>Redirect</type>
            </botNavigation>
            <type>Navigation</type>
        </botSteps>
